<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/relativeLayout5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/NeoBG"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <!--RELATIVE 1-->
        <RelativeLayout
            android:id="@+id/rel1"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_100sdp"
            android:baselineAligned="false">

            <soup.neumorphism.NeumorphCardView

                style="@style/Widget.Neumorph.CardView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:layout_marginEnd="@dimen/_20sdp"
                android:padding="12dp"
                app:neumorph_backgroundColor="#145dfa"
                app:neumorph_shadowColorDark="@color/NeoShadow"
                app:neumorph_shadowColorLight="@color/NeoLightShadow"
                app:neumorph_shadowElevation="3dp"
                app:neumorph_shapeAppearance="@style/CustomShapeAppearance"
                app:neumorph_shapeType="flat"
                app:neumorph_strokeColor="@color/NeoBG"
                app:neumorph_strokeWidth="2dp">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/textmode"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginStart="@dimen/_14sdp"
                        android:layout_marginTop="@dimen/_13sdp"
                        android:fontFamily="@font/nunito_sans_italic"
                        android:text="DEFAULT"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_23sdp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/textmode"
                        android:layout_alignParentStart="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginStart="@dimen/_14sdp"
                        android:fontFamily="@font/poesten"
                        android:text="Azure App made life easier"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_8sdp"
                        android:textStyle="normal" />

                    <com.airbnb.lottie.LottieAnimationView
                        android:id="@+id/lottne1"
                        android:layout_width="@dimen/_70sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:layout_centerVertical="true"
                        android:layout_toStartOf="@id/defaultprofileneoui"
                        android:elevation="4dp"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_rawRes="@raw/questionmark"
                        app:lottie_speed="1" />

                    <soup.neumorphism.NeumorphCardView
                        android:id="@+id/defaultprofileneoui"
                        style="@style/Widget.Neumorph.CardView"
                        android:layout_width="@dimen/_70sdp"
                        android:layout_height="@dimen/_70sdp"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:padding="12dp"
                        app:neumorph_backgroundColor="#145dfa"
                        app:neumorph_shadowColorDark="#114fd5"
                        app:neumorph_shadowColorLight="#176bff"
                        app:neumorph_shadowElevation="3dp"
                        app:neumorph_shapeAppearance="@style/customshapeimg"
                        app:neumorph_shapeType="flat"
                        app:neumorph_strokeColor="#145dfa"
                        app:neumorph_strokeWidth="2dp">

                        <ImageView
                            android:id="@+id/defaultprofile"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:contentDescription="TODO"
                            android:src="@drawable/azurenewlogo_round" />


                    </soup.neumorphism.NeumorphCardView>


                </RelativeLayout>

            </soup.neumorphism.NeumorphCardView>
        </RelativeLayout>
        <!--RELATIVE 1-->


        <!--RELATIVE 2-->
        <RelativeLayout
            android:id="@+id/rel2"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_250sdp"
            android:layout_below="@id/rel1"
            android:layout_marginTop="-3dp">


            <RelativeLayout
                android:id="@+id/relnew1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_125sdp">

                <soup.neumorphism.NeumorphCardView
                    android:id="@+id/carview1"
                    style="@style/Widget.Neumorph.CardView"
                    android:layout_width="@dimen/_120sdp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginStart="@dimen/_35sdp"
                    android:padding="12dp"
                    app:neumorph_backgroundColor="@color/NeoBG"
                    app:neumorph_shadowColorDark="@color/NeoShadow"
                    app:neumorph_shadowColorLight="@color/NeoLightShadow"
                    app:neumorph_shadowElevation="3dp"
                    app:neumorph_shapeAppearance="@style/CustomShapeAppearance"
                    app:neumorph_shapeType="flat"
                    app:neumorph_strokeColor="@color/NeoBG"
                    app:neumorph_strokeWidth="2dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:id="@+id/chillimage"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_marginHorizontal="@dimen/_10sdp"
                            android:layout_marginVertical="@dimen/_10sdp"
                            android:contentDescription="TODO"
                            android:src="@drawable/chillvector"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:id="@+id/chilltext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="@dimen/_55sdp"
                            android:fontFamily="@font/raleway_light"
                            android:text="CHILL"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_15sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/chillstatustextactived"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/chilltext"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:fontFamily="@font/raleway_light"
                            android:text="Tap to enable"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_8sdp"
                            android:textStyle="bold" />

                        <ImageView
                            android:id="@+id/chillstatus"
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:layout_alignParentEnd="true"
                            android:layout_marginVertical="@dimen/_10sdp"
                            android:layout_marginEnd="@dimen/_5sdp"
                            android:contentDescription="TODO"
                            android:src="@drawable/defaultoff"
                            tools:ignore="ContentDescription" />


                    </RelativeLayout>


                </soup.neumorphism.NeumorphCardView>

                <soup.neumorphism.NeumorphCardView
                    android:id="@+id/carview2"
                    style="@style/Widget.Neumorph.CardView"
                    android:layout_width="@dimen/_120sdp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_toEndOf="@id/carview1"
                    android:padding="12dp"
                    app:neumorph_backgroundColor="@color/NeoBG"
                    app:neumorph_shadowColorDark="@color/NeoShadow"
                    app:neumorph_shadowColorLight="@color/NeoLightShadow"
                    app:neumorph_shadowElevation="3dp"
                    app:neumorph_shapeAppearance="@style/CustomShapeAppearance"
                    app:neumorph_shapeType="flat"
                    app:neumorph_strokeColor="@color/NeoBG"
                    app:neumorph_strokeWidth="2dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:id="@+id/smartimg"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_marginHorizontal="@dimen/_10sdp"
                            android:layout_marginVertical="@dimen/_10sdp"
                            android:contentDescription="TODO"
                            android:src="@drawable/smartvector"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:id="@+id/smarttext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="@dimen/_55sdp"
                            android:fontFamily="@font/raleway_light"
                            android:text="Smart Casual"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_15sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/smartstatustextactived"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/smarttext"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:fontFamily="@font/raleway_light"
                            android:text="Tap to enable"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_8sdp"
                            android:textStyle="bold" />

                        <ImageView
                            android:id="@+id/smartstatus"
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:layout_alignParentEnd="true"
                            android:layout_marginVertical="@dimen/_10sdp"
                            android:layout_marginEnd="@dimen/_5sdp"
                            android:contentDescription="TODO"
                            android:src="@drawable/defaultoff"
                            tools:ignore="ContentDescription" />
                    </RelativeLayout>
                </soup.neumorphism.NeumorphCardView>


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relnew2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@id/relnew1">

                <soup.neumorphism.NeumorphCardView
                    android:id="@+id/carview3"
                    style="@style/Widget.Neumorph.CardView"
                    android:layout_width="@dimen/_120sdp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginStart="@dimen/_35sdp"
                    android:padding="12dp"
                    app:neumorph_backgroundColor="@color/NeoBG"
                    app:neumorph_shadowColorDark="@color/NeoShadow"
                    app:neumorph_shadowColorLight="@color/NeoLightShadow"
                    app:neumorph_shadowElevation="3dp"
                    app:neumorph_shapeAppearance="@style/CustomShapeAppearance"
                    app:neumorph_shapeType="flat"
                    app:neumorph_strokeColor="@color/NeoBG"
                    app:neumorph_strokeWidth="2dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:id="@+id/lazyimg"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_marginHorizontal="@dimen/_10sdp"
                            android:layout_marginVertical="@dimen/_10sdp"
                            android:contentDescription="TODO"
                            android:src="@drawable/lazyvector"
                            android:visibility="invisible"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:id="@+id/lazytext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="@dimen/_55sdp"
                            android:fontFamily="@font/raleway_light"
                            android:gravity="center"
                            android:text="COMING SOON !!"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_15sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/lazystatustextactived"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/lazytext"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:fontFamily="@font/raleway_light"
                            android:text="Tap to enable"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_8sdp"
                            android:textStyle="bold"
                            android:visibility="gone" />

                        <ImageView
                            android:id="@+id/lazystatus"
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:layout_alignParentEnd="true"
                            android:layout_marginVertical="@dimen/_10sdp"
                            android:layout_marginEnd="@dimen/_5sdp"
                            android:contentDescription="TODO"
                            android:src="@drawable/defaultoff"
                            android:visibility="invisible"
                            tools:ignore="ContentDescription" />

                    </RelativeLayout>
                </soup.neumorphism.NeumorphCardView>

                <soup.neumorphism.NeumorphCardView
                    android:id="@+id/carview4"
                    style="@style/Widget.Neumorph.CardView"
                    android:layout_width="@dimen/_120sdp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_toEndOf="@id/carview3"
                    android:padding="12dp"
                    app:neumorph_backgroundColor="@color/NeoBG"
                    app:neumorph_shadowColorDark="@color/NeoShadow"
                    app:neumorph_shadowColorLight="@color/NeoLightShadow"
                    app:neumorph_shadowElevation="3dp"
                    app:neumorph_shapeAppearance="@style/CustomShapeAppearance"
                    app:neumorph_shapeType="flat"
                    app:neumorph_strokeColor="@color/NeoBG"
                    app:neumorph_strokeWidth="2dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:id="@+id/fastafimg"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_marginHorizontal="@dimen/_10sdp"
                            android:layout_marginVertical="@dimen/_10sdp"
                            android:contentDescription="TODO"
                            android:src="@drawable/speed"
                            tools:ignore="ContentDescription" />


                        <TextView
                            android:id="@+id/fastaftext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="@dimen/_55sdp"
                            android:fontFamily="@font/raleway_light"
                            android:text="Fast AF"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_15sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/faststatustextactived"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/fastaftext"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:fontFamily="@font/raleway_light"
                            android:text="Tap to enable"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_8sdp"
                            android:textStyle="bold" />

                        <ImageView
                            android:id="@+id/faststatus"
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:layout_alignParentEnd="true"
                            android:layout_marginVertical="@dimen/_10sdp"
                            android:layout_marginEnd="@dimen/_5sdp"
                            android:contentDescription="TODO"
                            android:src="@drawable/defaultoff" />

                    </RelativeLayout>

                </soup.neumorphism.NeumorphCardView>


            </RelativeLayout>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/rel2" >

            <soup.neumorphism.NeumorphCardView

                style="@style/Widget.Neumorph.CardView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:padding="12dp"
                app:neumorph_backgroundColor="#323438"
                app:neumorph_shadowColorDark="@color/NeoShadow"
                app:neumorph_shadowColorLight="@color/NeoLightShadow"
                app:neumorph_shadowElevation="3dp"
                app:neumorph_shapeAppearance="@style/CustomShapeAppearance"
                app:neumorph_shapeType="flat"
                app:neumorph_strokeColor="@color/Teal"
                app:neumorph_strokeWidth="2dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:id="@+id/deviceName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_22sdp"
                        android:text="Device Name"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_10ssp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/ProcType"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/deviceName"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:text="Processor Type"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_10ssp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/silvercoreFreq"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/ProcType"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_17sdp"
                        android:text="Silver Core Max Freq"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_10ssp"
                        android:textStyle="bold" />

                <TextView
                    android:layout_below="@id/silvercoreFreq"
                    android:id="@+id/goldcoreFreq"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Gold Core Max Freq"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:textSize="@dimen/_10ssp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>

                    <TextView
                        android:layout_below="@id/goldcoreFreq"
                        android:id="@+id/primecoreFreq"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Prime Core Max Freq"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="@dimen/_10ssp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_marginLeft="@dimen/_10sdp"/>

                    <TextView
                        android:layout_below="@id/primecoreFreq"
                        android:id="@+id/gpumaxFreq"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="GPU Max Freq"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="@dimen/_10ssp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_10sdp"/>

                    <TextView
                        android:layout_below="@id/gpumaxFreq"
                        android:id="@+id/memFree"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Memory Free"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="@dimen/_10ssp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_10sdp"/>

                    <TextView
                        android:layout_below="@id/memFree"
                        android:id="@+id/memoryUsed"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Memory Used"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="@dimen/_10ssp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_marginLeft="@dimen/_10sdp"/>


                    <TextView
                        android:layout_below="@id/memoryUsed"
                        android:id="@+id/BatteryTemp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Battery Temp"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="@dimen/_10ssp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_10sdp"/>

                    <TextView
                        android:id="@+id/CPUtemp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/BatteryTemp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:text="CPU temp"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_10ssp"
                        android:textStyle="bold" />


                </RelativeLayout>
            </soup.neumorphism.NeumorphCardView>
            <RelativeLayout
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_30sdp"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_24sdp"
                android:background="@drawable/roundcorner_bluewhale">

                <ImageView
                    android:layout_centerVertical="true"
                    android:id="@+id/systemimg1"
                    android:layout_width="@dimen/_18sdp"
                    android:layout_height="@dimen/_18sdp"
                    android:src="@drawable/system" />

                <TextView
                    android:textStyle="bold"
                    android:layout_centerVertical="true"
                    android:id="@+id/sisinfo1"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@id/systemimg1"
                    android:text="SYSTEM INFO    "
                    android:textColor="@color/black"
                    android:textSize="@dimen/_12ssp" />
            </RelativeLayout>
        </RelativeLayout>


    </RelativeLayout>
    <!--RELATIVE 4-->
</androidx.constraintlayout.widget.ConstraintLayout>